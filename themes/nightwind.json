      }
    },
    {
      "name": "Markup Styling",
      "scope": "markup.bold, markup.italic",
      "settings": {
        "foreground": "#faf5ff"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#EC0095"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#ec4899"
      }
